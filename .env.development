# Development environment
NODE_ENV=development
PORT=3000

# Database Configuration (matches docker-compose.yml)
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=nfkbo
DB_SYNCHRONIZE=true
DB_LOGGING=true

# Authentication Configuration
# Bootstrap key for dev: nfkbo_dev_bootstrap_key_12345678901234567890123456789012
BOOTSTRAP_API_KEY=nfkbo_dev_bootstrap_key_12345678901234567890123456789012
RATE_LIMIT_WINDOW_MS=60000

# KBO API Configuration
USE_REAL_KBO_PROVIDER=false